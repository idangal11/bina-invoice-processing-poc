# ============================
# LLM Configuration
# ============================

# Whether to use a real LLM or mock mode
# true  = use Anthropic / real LLM
# false = run in mock / deterministic mode
USE_LLM=true

# LLM provider model name
# Example: claude-3-5-haiku-20241022
LLM_MODEL=claude-3-5-haiku-20241022

# Anthropic API key
# Replace with your own key when running locally
ANTHROPIC_API_KEY=YOUR_ANTHROPIC_API_KEY_HERE
